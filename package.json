{
  "name": "src",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp --env development",
    "build": "gulp build --env production",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "assets/js/*.js": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ],
    "assets/scss/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 1% in JP"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": false
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:all"
    ],
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "rules": {
      "no-var": "error",
      "one-var": "off",
      "no-implicit-globals": 0,
      "func-style": 0,
      "padded-blocks": 0,
      "new-cap": 0,
      "no-magic-numbers": 0,
      "no-plusplus": 0,
      "strict": 0,
      "sort-vars": 0,
      "max-len": 0,
      "no-use-before-define": 0,
      "max-statements-per-line": 0,
      "id-length": 0,
      "camelcase": 0,
      "no-negated-condition": 0,
      "init-declarations": 0,
      "no-loop-func": 0,
      "no-inline-comments": 0,
      "line-comment-position": 0,
      "eqeqeq": 0,
      "max-statements": 0,
      "no-shadow": 0
    },
    "globals": {
      "window": true,
      "console": true,
      "document": true,
      "setInterval": true,
      "setTimeout": true,
      "performance": true,
      "XMLHttpRequest": true,
      "location": true
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.7",
    "eslint": "^5.16.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-csscomb": "^3.0.8",
    "gulp-csv2json": "^0.2.5",
    "gulp-ejs": "^5.1.0",
    "gulp-eslint": "^5.0.0",
    "gulp-html-beautify": "^1.0.1",
    "gulp-notify": "^3.2.0",
    "gulp-parcel": "^0.3.5",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "install": "^0.11.0",
    "lint-staged": "^8.1.3",
    "npm": "^5.8.0",
    "prettier": "1.17.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "stylelint": "^10.0.0",
    "stylelint-scss": "^3.5.3",
    "typescript": "^4.7.4",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "dependencies": {
    "gulp": "^4.0.2"
  }
}
