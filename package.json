{
  "name": "src",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp --env development",
    "build": "gulp build --env production",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "assets/js/*.js": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ],
    "assets/scss/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 1% in JP"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": false
  },
  "stylelint": {
    "extends": "@geekcojp/stylelint-config"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "@geekcojp/eslint-config"
    ],
    "rules": {
      "object-curly-newline": [
        "error",
        "always"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@geekcojp/eslint-config": "^0.1.2",
    "@geekcojp/stylelint-config": "^0.2.3",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.13.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-ejs": "^5.1.0",
    "gulp-csscomb": "^3.0.8",
    "gulp-csv2json": "^0.2.5",
    "gulp-eslint": "^5.0.0",
    "gulp-notify": "^3.2.0",
    "gulp-parcel": "^0.3.5",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-html-beautify": "^1.0.1",
    "install": "^0.11.0",
    "browser-sync": "^2.26.7",
    "lint-staged": "^8.1.3",
    "npm": "^5.8.0",
    "prettier": "1.17.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "stylelint": "^10.0.0",
    "stylelint-scss": "^3.5.3"
  },
  "dependencies": {
    "gulp": "^4.0.2"
  }
}
